{
  "name": "viage",
  "version": "1.3.17",
  "description": "Modern, minimal, fast web framework",
  "main": "./dist/index.js",
  "homepage": "https://github.com/schlotg/viage",
  "types": "./dist/types/index.d.ts",
  "scripts": {
    "build": "node \"rm-dist\" && node build && tsc",
    "test": "jest",
    "pub": "npm run build && npm publish"
  },
  "keywords": [
    "Viage",
    "Javascript Framework"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/schlotg/viage"
  },
  "author": "Greg Schlottmann",
  "license": "MIT",
  "dependencies": {
    "fs-jetpack": "^2.2.0",
    "typescript": "^2.9.2"
  },
  "devDependencies": {
    "@types/jest": "^23.3.10",
    "jest": "^23.6.0",
    "ts-jest": "^23.10.5"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
