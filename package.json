{
  "name": "viage",
  "version": "1.3.2",
  "description": "Modern, minimal, fast web framework",
  "main": "./dist/index.js",
  "homepage": "https://github.com/schlotg/viage",
  "types": "./dist/types/index.d.ts",
  "scripts": {
    "build": "node \"rm-dist\" && tsc",
    "test": "jest"
  },
  "keywords": [
    "Viage",
    "Javascript Framework"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/schlotg/viage"
  },
  "author": "Greg Schlottmann",
  "license": "MIT",
  "dependencies": {
    "fs-jetpack": "^2.1.0",
    "typescript": "^2.8.1"
  },
  "devDependencies": {
    "@types/jest": "^23.1.6",
    "jest": "^23.4.1",
    "ts-jest": "^23.0.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
